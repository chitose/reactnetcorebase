{
    "ConnectionStrings": {
        "DefaultConnection": "* set from usersecret or environment config *"
    },
  "Settings": {
    "PasswordOptions": {
      "RequireDigit": true,
      "RequiredLength": 8,
      "RequireLowercase": true,
      "RequireNonAlphanumeric": true,
      "RequireUppercase": true
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}